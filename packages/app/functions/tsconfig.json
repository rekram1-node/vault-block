{
    "extends": "../tsconfig.json",
    "include": [
        "./api/**/*",
        "./src/**/*",
        "../shared/**/*",
        "types",
    ],
    "compilerOptions": {
        "composite": true,
        "tsBuildInfoFile": "../node_modules/.tmp/backend/tsconfig.tsbuildinfo",
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "outDir": "../node_modules/.tmp/backend",
        "rootDir": "..",
        "lib": [
            "ESNext"
        ],
        "types": [
            "@cloudflare/workers-types",
         ],
    }
}